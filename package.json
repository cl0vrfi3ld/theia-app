{
  "name": "theia-downloader",
  "description": "A cool little youtube downloader",
  "repository": "https://github.com/cl0vrfi3ld/theia-app",
  "author": {
    "name": "Cl0vr",
    "url": "https://cl0vr.co/",
    "email": "47996003+cl0vrfi3ld@users.noreply.github.com"
  },
  "version": "0.2.0",
  "private": true,
  "main": "src/background/main.js",
  "homepage": "https://theia.cl0vr.co",
  "license": "MIT",
  "scripts": {
    "build-js": "cross-env NODE_ENV=production rollup -c",
    "pack": "yarn build-js && electron-builder --dir",
    "dist": " yarn build-js && electron-builder",
    "release": "source ci.env && electron-builder",
    "full-send": "yarn build-js && yarn release",
    "full-send-pre": "yarn build-js && cross-env EP_PRE_RELEASE=true yarn release",
    "publish:dev": "source .local.env && yarn dist --publish always --config.publish.provider=s3 --config.publish.endpoint=http://127.0.0.1:9000  --config.publish.bucket=theia-downloader-updates.test",
    "local-s3": "docker compose -f ./utils/local-s3.docker-compose.yml up",
    "increment-version": "versiony package.json --patch",
    "bump-version": "versiony package.json --patch && git add . && git commit -m 'new version' && git push",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "start": "electron .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "electron-builder install-app-deps",
    "tag-pre": "",
    "tag": ""
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.2",
    "@rollup/plugin-commonjs": "18",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-builder-notarize": "^1.5.1",
    "inquirer": "^8.2.0",
    "node-fetch": "^3.1.0",
    "postcss": "^8",
    "postcss-load-config": "^3.1.0",
    "postcss-nesting": "7",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.2.10",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.0.1",
    "toml": "^3.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "electron-log": "4",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "gun": "^0.2020.1235",
    "lodash": "^4.17.21",
    "sass": "^1.45.0",
    "sirv-cli": "^1.0.0",
    "svelte-spa-router": "^3.2.0",
    "ytcog": "^2.4.3"
  }
}
